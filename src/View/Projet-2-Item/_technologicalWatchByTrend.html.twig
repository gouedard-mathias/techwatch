<div class = 'carrouselHome'>
    {% for article in articleTrend %}
        <div class="card" style="width: 18rem;">
            <img src="{{ article.image_url }}" class="card-img-top" alt="image random">
            <div class="card-body">
                <h5 class="card-title">{{ article.title }}</h5>
            </div>
        </div>
    {% endfor %}
</div>